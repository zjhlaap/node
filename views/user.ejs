                      <h3 class="h4">昵称</h3><small>关注: 32</small>
                    </div>
                  </div>
                  <div class="project-date"><span class="hidden-sm-down">注册时间 3月23号</span></div>
                </div>
                <div class="right-col col-lg-6 d-flex align-items-center">
                  <div class="time">
                    <a name="" id="" class="btn btn-danger small" href="#" role="button">删除</a>
                  </div>
                  <div class="comments">
                    <a name="" id="" class="btn btn-dark small" href="user_add.html">修改</a>
                  </div>
                  <div class="project-progress">
                    <div class="progress">
                      <div role="progressbar" style="width: 45%; height: 6px;" aria-valuenow="25" aria-valuemin="0"
                        aria-valuemax="100" class="progress-bar bg-red"></div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <%}%>
            <div class="project">
              <% include ./public/paging.ejs %>
            </div>
          </div>
        </section>
        <!-- Page Footer-->
        <% include ./public/footer.ejs %>
      </div>
    </div>
  </div>
